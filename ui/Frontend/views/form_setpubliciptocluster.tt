<form method="POST" action="">
<table class="form">	
	<tr><td><select name="publicip_id">
	<% FOREACH ip IN FREEPUBLICIPS %>
		<option value="<% ip.PUBLICIP_ID %>">
			<% ip.IP_ADDRESS %>
			 | <% ip.IP_MASK %>
			 | <% ip.GATEWAY %>
		</option>
	<% END %>	
	</select></td>
	</tr>
</table>
<div class="buttons">
	<input type="hidden" name="cluster_id" value="<% cluster_id %>" />
	<input type="submit" value="ok"/>
</div>
</form> 


