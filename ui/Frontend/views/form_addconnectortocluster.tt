<form id="target" method="POST" action="/architectures/extclusters/<% cluster_id %>/connectors/add">
<table class="form">
	<tr>
		<td class="label">available connectors :</td>
		<td><select name="connector_type_id">
		<% FOREACH conn IN connectors_list %>
			<option value="<% conn.connector_type_id %>"><% conn.connector_name %> - <% conn.connector_category %></option>
		<% END %>
		</select>
		</td>
	</tr>
</table>
<input type="hidden" name="cluster_id" value="<% cluster_id %>" />
</form>
