<TMPL_INCLUDE NAME="header.tmpl">
<TMPL_INCLUDE NAME="menu.tmpl">
<style type="text/css">
@import url(/css/listing.css);
</style>
<div id="content">
<table class="simplelisting">
	<caption>Operations Queue</caption>
	<tr><th></th><th>type</th><th>by</th><th>priority</th><th>rank</th><th>creation</th></tr>
	<TMPL_LOOP NAME=OPERATIONS>
	<tr <TMPL_IF NAME=RANK><TMPL_ELSE>style="background-color:#0F0"</TMPL_IF>>
		<td id="m<TMPL_VAR NAME=ID>" class="expanddown" style="cursor:pointer;text-align:center">+</td>
		<td class="bold"><TMPL_VAR NAME=TYPE></td>
		<td class="center"><TMPL_VAR NAME=FROM></td>
		<td class="center"><TMPL_VAR NAME=PRIORITY></td>
		<td class="center"><TMPL_VAR NAME=RANK></td>
		<td class="center"><TMPL_VAR NAME=CREATION></td>
	</tr>
	<tr id="t<TMPL_VAR NAME=ID>" class="hidden">
		<td colspan="8">
			<table style="margin-left: 40px;">	
				<TMPL_LOOP NAME=PARAMETERS>
				<tr style="margin:0;"><td class="right" style="border:none;padding-top:0;"><TMPL_VAR NAME=PARAMNAME> : </td><td class="content"  style="border:none;padding-top:0;"><TMPL_VAR NAME=VAL></td></tr>
				</TMPL_LOOP>
			</table>
		</td>
	</tr>
</TMPL_LOOP>
</table>
</div>
<script language="Javascript" src="/js/listingtools.js"></script>
<script language="Javascript" type="text/javascript">
var t=setTimeout("window.location.reload();",5000);
</script>
<TMPL_INCLUDE NAME="footer.tmpl">
