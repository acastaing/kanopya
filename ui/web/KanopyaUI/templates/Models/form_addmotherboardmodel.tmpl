<html>
<head>
	<title>new motherboard model</title>
</head>
<body>
<style type="text/css">@import url(/css/form.css);</style>
<div class="title">create model of motherboard<TMPL_IF NAME=some_errors><span class="error">Some fields below are missing or invalid</span></TMPL_IF></div>
<form method="POST" action="/cgi/kanopya.cgi/models/process_addmotherboardmodel">
<table class="form">
	<tr>
		<td class="label">Brand :</td>
		<td><input type="text" name="brand" size="20"/><TMPL_VAR NAME=ERR_BRAND></td>
	</tr>
	<tr>
		<td class="label">Name :</td>
		<td><input type="text" name="name" size="20" /><TMPL_VAR NAME=ERR_NAME></td>
	</tr>
	<tr>
		<td class="label">Chipset :</td>
		<td><input type="text" name="chipset" size="10" /><TMPL_VAR NAME=ERR_CHIPSET></td>
	</tr>
	<tr>
		<td class="label">Processors number :</td>
		<td><input type="text" name="procnum" size="10" /><TMPL_VAR NAME=ERR_PROCNUM></td>
	</tr>
	<tr>
		<td class="label">Consumption (W):</td>
		<td><input type="text" name="consumption" size="10" /><TMPL_VAR NAME=ERR_CONSUMPTION></td>
	</tr>
	<tr>
		<td class="label">iface number :</td>
		<td><input type="text" name="ifacenum" size="10" /><TMPL_VAR NAME=ERR_IFACENUM></td>
	</tr>
	<tr>
		<td class="label">RAM slots number :</td>
		<td><input type="text" name="ramslotnum" size="10" /><TMPL_VAR NAME=ERR_RAMSLOTNUM></td>
	</tr>
	<tr>
		<td class="label">RAM max (GB):</td>
		<td><input type="text" name="rammax" size="4" /><TMPL_VAR NAME=ERR_RAMMAX></td>
	</tr>
	<tr>
		<td class="label">solded processor :</td>
		<td><select name="processorid">
			<option selected="selected" value="0">no</option>
			<TMPL_LOOP NAME=processormodels_list>
			<option value="<TMPL_VAR NAME=processormodel_id>"><TMPL_VAR NAME=processormodel_name></option>
			</TMPL_LOOP>
		</select></td>
	</tr>
</table>
<div class="buttons">
<input type="submit" value="ok"/>
<input type="button" value="cancel" onclick="javascript:window.close()"/>
</div>
</form> 
</body>	
</html>


