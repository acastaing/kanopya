<div id="content">
<table class="simplelisting">
	<caption>Motherboard Models</caption>
	<tr><th>model</th><th>chipset</th><th>processors<br/>number</th><th>consumption</th><th>iface<br/>number</th><th>RAM<br/>slots</th><th>RAM<br/>max</th><th></th></tr>
	<TMPL_LOOP NAME=motherboardmodels_list>
	<td><TMPL_VAR NAME=mmodel_brand> <span class="bold"><TMPL_VAR NAME=mmodel_name></span></td>
	<td><TMPL_VAR NAME=mmodel_chipset></td>
	<td class="right"><TMPL_VAR NAME=mmodel_processornum></td>
	<td class="right"><TMPL_VAR NAME=mmodel_consumption> W</td>
	<td class="right"><TMPL_VAR NAME=mmodel_ifacenum></td>
	<td class="right"><TMPL_VAR NAME=mmodel_ramslotnum></td>
