	<TMPL_IF NAME=can_setperm><br /><a class="imgPermission" href="/cgi/kanopya.cgi/permissions/view_selectconsumer?entitytype=Motherboardmodel&id=<TMPL_VAR NAME=mmodel_id>">permissions</a></TMPL_IF>
