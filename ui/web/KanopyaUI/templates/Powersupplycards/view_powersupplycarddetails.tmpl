<TMPL_INCLUDE NAME="header.tmpl">
<TMPL_INCLUDE NAME="menu.tmpl">
<style type="text/css">@import url(/css/details.css);</style>
<script type="text/javascript" src="/js/extern/jquery.js"></script>
<script type="text/javascript" src="/js/details.js"></script>

<div id="content">
<h2>Power supply card : <TMPL_VAR NAME=powersupplycard_name></h2>
<hr/>
<span id="state" class="title simpleexpand">State</span>
<table id="Xstate" class="detailspart">
<tr><TMPL_IF NAME=active><td>Active</td><TMPL_ELSE><td>Deactivated</td></TMPL_IF>
<td class="actions">
<TMPL_IF NAME=can_activate>
<a class="imgActivate" href="/cgi/kanopya.cgi/powersupplycards/process_activatepowersupplycard?powersupplycard_id=<TMPL_VAR NAME=powersupplycard_id>">activate</a><br />
<span class="desc">Activate this power supply card.</span><br /><br />
</TMPL_IF>
<TMPL_IF NAME=can_deactivate>
<a class="imgDeactivate" href="/cgi/kanopya.cgi/powersupplycards/process_deactivatepowersupplycard?powersupplycard_id=<TMPL_VAR NAME=powersupplycard_id>">deactivate</a><br />
<span class="desc">Deactivate this cluster.</span><br /><br />
</TMPL_IF>
</td></tr>
</table>

<hr />
<span id="mainsettings" class="title simpleexpand">Main settings</span>
<table id="Xmainsettings" class="detailspart">
<tr><td class="italic" colspan="2"><TMPL_VAR NAME=powersupplycard_desc></td>
<td class="actions" rowspan="6">
<a class="imgEdit" href="javascript:openform('/cgi/kanopya.cgi/powersupplycards/form_editpowersupplycard?powersupplycard_id=<TMPL_VAR NAME=powersupplycard_id>', 'edit power supply card', 800, 400, '');">edit</a><br />
<span class="desc">Modify these global parameters.</span><br /><br />
<TMPL_IF NAME=can_delete>
<a class="imgRemove" href="/cgi/kanopya.cgi/powersupplycards/process_removepowersupplycard?powersupplycard_id=<TMPL_VAR NAME=powersupplycard_id>">delete</a><br />
<span class="desc">Delete this power supply card.</span><br /><br /></TMPL_IF>
<TMPL_IF NAME=can_setperm>
<a class="imgPermission" href="/cgi/kanopya.cgi/permissions/view_selectconsumer?entitytype=Powersupplycard&id=<TMPL_VAR NAME=powersupplycard_id>">permission</a><br />
<span class="desc">View/set permissions on this power supply card.</span><br /></TMPL_IF>
</td><tr/>
<tr><td colspan="2">&nbsp;</td></tr>
<tr><td class="label">Model :</td><td><TMPL_VAR NAME=model></td></tr>
<tr><td class="label">Slots count :</td><td><TMPL_VAR NAME=slots_count></td></tr>
<tr><td class="label">MAC address :</td><td><TMPL_VAR NAME=mac_address></td></tr>
<tr><td class="label">IP address :</td><td><TMPL_VAR NAME=ip></td></tr>
</table>

<hr/>
<span id="nodes" class="title simpleexpand">Power Slots</span>
<table id="Xnodes" class="detailspart list">
<tr><td>...</td></tr>
</table>

</div>
