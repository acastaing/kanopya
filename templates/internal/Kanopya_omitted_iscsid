#!/bin/bash
# This file is generated by Kanopya Private Cloud
# Please Do not modify it directly.
# This script allows to record iscsi record to not be closed when poweroff the host
ps --sort pid -C sshd -o pid= | sed 's/\s\(.*\)/\1/' >> /var/run/sendsigs.omit
ps -C iscsid -o pid= | sed 's/\s\(.*\)/\1/' >> /var/run/sendsigs.omit
