<TMPL_INCLUDE NAME="header.tmpl">
<TMPL_INCLUDE NAME="menu.tmpl">
<style type="text/css">@import url(/css/form.css);</style>

<div id="content">
<form method="POST" action="/cgi/mcsui.cgi/models/process_addmotherboardmodel">
<table class="form">
	<caption>New motherboard model</caption>
	<TMPL_IF NAME=SOME_ERRORS>
	<tr><td class="error" colspan="2">Some fields below are missing or invalid</td></tr>
	</TMPL_IF>
	<tr>
		<td class="label">Brand :</td>
		<td><input type="text" name="brand" size="20"/><TMPL_VAR NAME=ERR_BRAND></td>
	</tr>
	<tr>
		<td class="label">Name :</td>
		<td><input type="text" name="name" size="20" /><TMPL_VAR NAME=ERR_NAME></td>
	</tr>
	<tr>
		<td class="label">Chipset :</td>
		<td><input type="text" name="chipset" size="10" /></td>
	</tr>
	<tr>
		<td class="label">Processors number :</td>
		<td><input type="text" name="procnum" size="10" /></td>
	</tr>
	<tr>
		<td class="label">Consumption (W):</td>
		<td><input type="text" name="consumption" size="10" /><TMPL_VAR NAME=ERR_CONSUMPTION></td>
	</tr>
	<tr>
		<td class="label">iface number :</td>
		<td><input type="text" name="ifacenum" size="10" /></td>
	</tr>
	<tr>
		<td class="label">RAM slots number :</td>
		<td><input type="text" name="ramslotnum" size="10" /></td>
	</tr>
	<tr>
		<td class="label">RAM max (GB):</td>
		<td><input type="text" name="rammax" size="4" /></td>
	</tr>
	<tr>
		<td class="label">solded processor :</td>
		<td><select name="processorid">
			<option selected="selected" value="0">no</option>
			<TMPL_LOOP NAME=PROCMODEL>
			<option value="<TMPL_VAR NAME=ID>"><TMPL_VAR NAME=NAME></option>
			</TMPL_LOOP>
		</select><a class="simple" href="/cgi/mcsui.cgi/models/form_addprocessormodel">no found ?</a></td>
	</tr>
</table>
<div class="formbuttons">
	<input class="button" type="reset" value="clear" />
	<a class="button" href="/cgi/mcsui.cgi/models">cancel</a>
	<input class="button" type="submit" value="add" />
</div>
</form> 
</div>
<TMPL_INCLUDE NAME="footer.tmpl">

