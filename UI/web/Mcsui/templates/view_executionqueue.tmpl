<TMPL_INCLUDE NAME="header.tmpl">
<TMPL_INCLUDE NAME="menu.tmpl">
<style type="text/css">
@import url(/css/listing.css);
</style>
<div id="content">
<h1>Execution Queue</h1>
<table id="executionlist" class="listview">
	<tr><th></th><th>type</th><th>by</th><th>priority</th><th>rank</th><th>date</th><th>time</th></tr><TMPL_LOOP NAME=OPERATIONS>
	<tr>
		<td id="m<TMPL_VAR NAME=ID>" class="expand">+</td>
		<td><TMPL_VAR NAME=TYPE></td>
		<td><TMPL_VAR NAME=FROM></td>
		<td><TMPL_VAR NAME=PRIORITY></td>
		<td><TMPL_VAR NAME=RANK></td>
		<td><TMPL_VAR NAME=DATE></td>
		<td><TMPL_VAR NAME=TIME></td>
	</tr>
	<tr id="t<TMPL_VAR NAME=ID>" class="hidden">
		<td colspan="7">
			<table class="listdetails">	
				<TMPL_LOOP NAME=PARAMETERS>
				<tr><td class="label"><TMPL_VAR NAME=PARAMNAME> : </td><td class="content"><TMPL_VAR NAME=VAL></td></tr>
				</TMPL_LOOP>
			</table>
		</td>
	</tr>
</TMPL_LOOP>
</table>
<TMPL_LOOP NAME=OPERATIONSPARAMETERS>
<table id="l<TMPL_VAR NAME=ID>" class="listdetails hidden">
	<tr><td colspan="2"><hr/></td></tr>
	<tr><td class="label"><TMPL_VAR NAME=PARAMNAME></td><td class="content"><TMPL_VAR NAME=VAL></td></tr>
	<tr><td colspan="2"><hr/></td></tr>
</table>
</TMPL_LOOP>

</div>
<script language="Javascript">
function selectme() { 
	if(this.innerHTML == '+') { 
		this.innerHTML = '-'; 
		document.getElementById('t'+this.id.substring(1)).className = ''; 
	}
	else { 
		this.innerHTML = '+';
		document.getElementById('t'+this.id.substring(1)).className = 'hidden'; 
	}
}

motherboardline = document.getElementById('executionlist').getElementsByClassName('expand');
for(i=0;i<motherboardline.length;i++) {
	motherboardline[i].onclick = selectme;
}

</script>
<TMPL_INCLUDE NAME="footer.tmpl">
